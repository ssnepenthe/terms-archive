{
    "name": "ssnepenthe/terms-archive",
    "description": "A WordPress plugin for creating frontend terms archives.",
    "type": "wordpress-plugin",
    "homepage": "https://github.com/ssnepenthe/terms-archive",
    "license": "GPL-2.0",
    "require": {
        "php": ">=5.6",
        "composer/installers": "^1.0",
        "ssnepenthe/metis": "^0.6",
        "ssnepenthe/wp-requirements": "^0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "psy/psysh": "^0.9",
        "ssnepenthe/ssn-test-kit": "*"
    },
    "autoload": {
        "classmap": ["src/"]
    },
    "repositories": [
        {
            "type": "path",
            "url": "../../../../../../../../Code/ssn-test-kit/",
            "options": {
                "symlink": false
            }
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
